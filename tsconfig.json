{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
     "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx", "pages/albums/[id].tsx"
, "pages/archived/albums.js", 
"pages/api/user/createuser.tsx", 
"components/utilities/loading.js"
, "pages/about.js", "pages/_app.js"  ],
  "exclude": [
    "node_modules"
  ]
}
